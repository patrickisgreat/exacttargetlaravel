{
  "name": "digitaladditive/ExactTargetLaravel",
  "type": "library",
  "description": "The best way to talk to Exact Target with Hypertext PreProcessor",
  "license": "MIT",
  "keywords": [
    "digitaladditive",
    "ExactTargetLaravel",
    "Exact Target",
    "Laravel"
  ],
  "homepage": "https://github.com/:vendor/:package_name",
  "authors": [
    {
      "name": "Patrick Bennett",
      "email": "patrick.bennett@digitaladditive.com",
      "homepage": "http://digitaladditive.com",
      "role": "Developer"
    }
  ],
  "require": {
    "illuminate/support": "~5.0",
    "php": "^7.1.3",
    "guzzlehttp/guzzle": "~6.0",
    "digitaladditive/php-fuel-sdk-fork": ">=1.2",
    "symfony/dotenv": "^4.2"
  },
  "require-dev": {
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^2.0",
    "phpunit/phpunit": "^7.0",
    "laravel/homestead": "^3.0"
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "digitaladditive\\ExactTargetLaravel\\": "src"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/",
      "src/"
    ],
    "psr-4": {
      "digitaladditive\\ExactTargetLaravel\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
